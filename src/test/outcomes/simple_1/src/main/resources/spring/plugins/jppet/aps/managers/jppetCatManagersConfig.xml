<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:jee="http://www.springframework.org/schema/jee"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
			http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-3.0.xsd">


	<bean id="jppetCatDAO" class="org.entando.entando.plugins.jppet.aps.system.services.cat.CatDAO">
		<property name="dataSource" ref="servDataSource" />
	</bean>

	<bean id="jppetCatManager" class="org.entando.entando.plugins.jppet.aps.system.services.cat.CatManager"
		parent="abstractService">
		<property name="keyGeneratorManager" ref="KeyGeneratorManager" />
		<property name="catDAO" ref="jppetCatDAO" />
	</bean>

</beans>